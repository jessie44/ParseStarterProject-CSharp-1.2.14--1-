        <Storyboard x:Name="Storyboard1">
            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="submit">
                <DiscreteObjectKeyFrame KeyTime="0">
                    <DiscreteObjectKeyFrame.Value>
                        <Visibility>Visible</Visibility>
                    </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
            </ObjectAnimationUsingKeyFrames>
            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="border">
                <DiscreteObjectKeyFrame KeyTime="0">
                    <DiscreteObjectKeyFrame.Value>
                        <Visibility>Visible</Visibility>
                    </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
            </ObjectAnimationUsingKeyFrames>
            <DoubleAnimation Duration="0" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="submit" d:IsOptimized="True"/>
            <DoubleAnimation Duration="0" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="border" d:IsOptimized="True"/>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="submit">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-611"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="border">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-611"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimation Duration="0:0:1" To="-611" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="border1" d:IsOptimized="True"/>
        </Storyboard>
        <Storyboard x:Name="load" RepeatBehavior="3x">
            <DoubleAnimation Duration="0:0:0.6" To="360" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationY)" Storyboard.TargetName="border1" d:IsOptimized="True"/>
        </Storyboard>
        <Storyboard x:Name="bropop">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="webbrowser">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1443"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Page.Resources>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Image Source="Assets/wassup.png.jpg" Stretch="Fill"/>

        <Border x:Name="border" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="325" Margin="454,255,0,0" VerticalAlignment="Top" Width="467" RenderTransformOrigin="0.5,0.5">
            <Border.RenderTransform>
                <CompositeTransform/>
            </Border.RenderTransform>
            <Border.Projection>
                <PlaneProjection/>
            </Border.Projection>
        </Border>
        <TextBox x:Name="user" HorizontalAlignment="Left" Margin="529,347,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309" RenderTransformOrigin="0.5,0.5" PlaceholderText="Enter Username" FontStyle="Italic">
            <TextBox.RenderTransform>
                <CompositeTransform/>
            </TextBox.RenderTransform>
            <TextBox.Projection>
                <PlaneProjection/>
            </TextBox.Projection>
        </TextBox>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="529,404,0,0"  VerticalAlignment="Top" Width="309" RenderTransformOrigin="0.5,0.5" PlaceholderText="Enter Password" FontStyle="Italic" FontFamily="Global User Interface">
            <PasswordBox.RenderTransform>
                <CompositeTransform/>
            </PasswordBox.RenderTransform>
            <PasswordBox.Projection>
                <PlaneProjection/>
            </PasswordBox.Projection>
        </PasswordBox>
        <Button x:Name="submit" Content="Login" HorizontalAlignment="Left" Margin="526,463,0,0" VerticalAlignment="Top" Width="315" RenderTransformOrigin="0.5,0.5" Click="submit_Click">
            <Button.RenderTransform>
                <CompositeTransform/>
            </Button.RenderTransform>
            <Button.Projection>
                <PlaneProjection/>
            </Button.Projection>
        </Button>
        <Border x:Name="border1" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="325" Margin="454,256,0,0" VerticalAlignment="Top" Width="467" RenderTransformOrigin="0.5,0.5" Opacity="0">
            <Border.RenderTransform>
                <CompositeTransform/>
            </Border.RenderTransform>
            <Border.Projection>
                <PlaneProjection/>
            </Border.Projection>
            <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="Welcome " Margin="-9,61,-8,43" Width="464" Height="60" VerticalAlignment="Center" TextAlignment="Center" LineHeight="64" FontSize="32"/>
        </Border>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="346,847,0,-173" TextWrapping="Wrap" Text="So How Smart Are You?" VerticalAlignment="Top" Height="94" Width="700" FontSize="64" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
            <TextBlock.Projection>
                <PlaneProjection/>
            </TextBlock.Projection>
            <TextBlock.RenderTransform>
                <CompositeTransform Rotation="-12.269"/>
            </TextBlock.RenderTransform>
        </TextBlock>
        <Button x:Name="face" Content="Login With FaceBook" HorizontalAlignment="Left" Height="39" Margin="526,516,0,0" VerticalAlignment="Top" Width="312" Click="face_Click">
            <Button.Projection>
                <PlaneProjection/>
            </Button.Projection>
        </Button>
        <WebView x:Name="webbrowser" HorizontalAlignment="Left" Height="326" Margin="-1433,254,0,0" VerticalAlignment="Top" Width="1346" RenderTransformOrigin="0.5,0.5">
            <WebView.RenderTransform>
                <CompositeTransform/>
            </WebView.RenderTransform>
        </WebView>
        <ProgressRing x:Name="prog" HorizontalAlignment="Left" Margin="541.727,376.797,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="160.406" UseLayoutRounding="False" Width="271.546" d:LayoutRounding="Auto">
            <ProgressRing.RenderTransform>
                <CompositeTransform Rotation="-0.827"/>
            </ProgressRing.RenderTransform>
        </ProgressRing>

    </Grid>
</Page>
