    <Page.Resources>
        <Storyboard x:Name="success" Completed="sComplete">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationY)" Storyboard.TargetName="submit">
